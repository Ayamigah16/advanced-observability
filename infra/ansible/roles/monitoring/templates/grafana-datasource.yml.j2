# Grafana datasource provisioning â€” deployed by Ansible, loaded on startup
# Docs: https://grafana.com/docs/grafana/latest/administration/provisioning/#datasources
apiVersion: 1

datasources:
- name: Prometheus
  type: prometheus
  uid: prometheus-local
  url: http://localhost:9090
  access: proxy
  isDefault: true
  editable: false
  jsonData:
    timeInterval: "15s"
    httpMethod: POST
